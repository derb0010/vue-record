{
  "name": "@codekraft-studio/vue-record",
  "version": "0.0.3",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target lib --name VueRecord src/components/index.js",
    "lint": "vue-cli-service lint",
    "build:demo": "vue-cli-service build --dest demo",
    "prepublishOnly": "npm run build",
    "publish:demo": "git subtree push --prefix demo origin gh-pages"
  },
  "main": "dist/VueRecord.umd.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "core-js": "^3.8.3",
    "startaudiocontext": "^1.2.1",
    "tone": "^13.8.25",
    "vue": "^3.2.0",
    "@vue/compat": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-plugin-e2e-cypress": "^5.0.1",
    "@vue/cli-plugin-eslint": "^5.0.1",
    "@vue/cli-plugin-unit-jest": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "@vue/vue2-jest": "^27.0.0-alpha.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "cypress": "^8.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.1.0",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.0.0",
    "@vue/compiler-sfc": "^3.1.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged build"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}